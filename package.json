{
  "name": "livegraphs-static",
  "version": "0.1.0",
  "private": true,
  "description": "A web dashboard for visualizing chatbot conversation analytics",
  "homepage": "https://github.com/kjanat/livegraphs-static#readme",
  "bugs": {
    "url": "https://github.com/kjanat/livegraphs-static/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjanat/livegraphs-static.git"
  },
  "license": "AGPL-3.0-or-later",
  "author": "Kaj Kowalski <dev@kajkowalski.nl>",
  "scripts": {
    "build": "next build",
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "dev:turbo": "cross-env NODE_OPTIONS='--inspect' next dev --turbopack",
    "format": "biome format --write",
    "format:check": "biome format",
    "lint": "next lint",
    "lint:strict": "biome check",
    "pre-commit": "lint-staged",
    "prepare": "husky",
    "start": "pnpm dlx serve out #next start",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit",
    "validate": "pnpm lint && pnpm lint:strict && pnpm format:check && pnpm type-check"
  },
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "biome check --write --unsafe --no-errors-on-unmatched"
    ],
    "package.json": [
      "pnpm dlx sort-package-json"
    ]
  },
  "dependencies": {
    "@nivo/core": "^0.99.0",
    "@nivo/heatmap": "^0.99.0",
    "chart.js": "^4.5.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^4.1.0",
    "next": "^15.4.1",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "sql.js": "^1.13.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.2",
    "@eslint/eslintrc": "^3.3.1",
    "@next/eslint-plugin-next": "^15.4.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.0.14",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/sql.js": "^1.4.9",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.31.0",
    "eslint-config-next": "15.4.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "process": "^0.11.10",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad"
}
